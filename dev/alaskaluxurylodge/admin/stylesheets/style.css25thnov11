.sec_form .image_upload_div{float:right; width:200px;}


.main_footer{float:left; width:998px;  border: 1px solid #777777; background:url(../images/footer_bg.png) 0 0 repeat-x; height:40px; padding:25px 0 0; font-size:11px; color:#000000;}

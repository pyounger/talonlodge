.a_login_box .login_box ul li{float:left; width:220px; padding:0 0 5px;}
.a_login_box .login_box ul li label{float:left; width:85px; line-height:22px; font-size:13px; color:#fff; font-weight:bold; padding:0 5px;}
.a_login_box .login_box ul li label b{color:#FF0000;}
.a_login_box .login_box ul li  span{float:left; background:url(../images/txt_bg.png) 0 0 no-repeat; padding:3px; width:119px; height:19px;}
.a_login_box .login_box ul li  span input[type="text"]{float:left; width:119px; height:19px; background:none; border:none;}
.a_login_box .login_box ul li  span input[type="password"]{float:left; width:119px; height:19px; background:none; border:none;}
.a_login_box .login_box ul li .btn{float:right; background:url(../images/btn_login.png) 0 0 no-repeat; text-indent:-10000px; border:none; width:64px; height:25px;}

.a_login_box .error_msg{position:absolute; color:#FF0000; width:205px; top: -20px; background:#fff; left: 12px; font-size:11px; padding:5px; border:1px solid #FF0000;}

.divmargin{margin:0px 5px 0px 5px}
/*11-Nov-2011 Start New Css*/

.frame .main_content{float:left; width:978px; background:#fff; padding:0 10px; border-bottom:1px solid #777777;}
.main_content .main_admin{float:left; width:978px;}
.main_content .main_admin h1{float:left; padding-top:20px; padding-bottom:20px; width:978px; text-align:center; font-family:Arial, Helvetica, sans-serif; font-size:18px; font-weight:bold; line-height:25px;}
.main_admin .admin_main{ margin:auto; width:290px; }

.admin_main .admin_top{ float:left; width:290px; background:url(../images/top_admin.png) 0 top no-repeat; padding-top:15px;}
.admin_main .admin_bottom{ float:left; margin-bottom:20px; width:290px;background:url(../images/bottom_admin.png) 0 bottom no-repeat; padding-bottom:15px;}
.admin_main .admin_mid{ float:left; position:relative; width:290px; min-height:100px; background:url(../images/mid_admin.png) 0 0 repeat-y;}
.admin_mid .top_white{ position:absolute; width:285px; background:url(../images/top_white.png) 0 0 no-repeat; width:285px; top:-12px; left:2px; height:30px;}
.admin_mid .top_white h1{font-size:14px; width:270px; padding:0px 0px 0px 5px; text-align:left; font-weight:bold; color:#000; line-height:30px;font-family:Arial, Helvetica, sans-serif;}
.admin_main .close_icon{ position:absolute; width:32px; height:29px; right:3px; top:-12px; background:url(../images/close_icon.png) 0 0 no-repeat;}


.admin_mid .input_main{ float:left; width:290px; padding-top:20px;}
.admin_mid .input_main ul{ float:left; width:280px; padding-left:10px; list-style:none;}
.admin_mid .input_main ul li{float:left;}
.admin_mid .input_main ul li h6{float:left; font-size:18px; font-weight:bold; color:#000000; text-align:left;}
.admin_mid .input_main ul li label{float:left; width:270px; padding-bottom:5px; padding-top:5px; font-family:Arial, Helvetica, sans-serif; font-size:14px; color:#FFFFFF; font-weight:normal;}
.admin_mid .input_main ul li input[type="text"] { float:left; width:266px; height:26px; }
.admin_mid .input_main ul li span input[type="text"] { background:url(../images/input_bg.png) 0 0 no-repeat;}
.admin_mid .input_main ul li input[type="password"] { float:left; width:266px; height:26px; }
.admin_mid .input_main ul li span input[type="password"] { background:url(../images/input_bg.png) 0 0 no-repeat;}
.admin_mid .input_main ul li .div_btn{float:left; width:270px; padding-top:10px;}
.admin_mid .input_main ul li .div_btn ul{float:left; width:270px; list-style:none; padding-left:0px;}
.admin_mid .input_main ul li .div_btn ul li{float:left;}
.admin_mid .input_main ul li .div_btn ul li + li{float:left; padding-left:20px;}
.admin_mid .input_main ul li .div_btn ul li input[type=button]{float:left; cursor:pointer; border:none; padding:5px; background:#999999; font-family:Arial, Helvetica, sans-serif; font-size:14px; color:#000000; font-weight:bold;}
.admin_mid .input_main ul li .div_btn ul li input[type=button]:hover {background:#fff;}


